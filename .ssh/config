User agherzan
TCPKeepAlive yes
IdentitiesOnly yes

# Resin build server
Host resinbuild
Hostname build1.dev.resin.io
User andrei
IdentityFile ~/.ssh/id_rsa_resin
TCPKeepAlive yes
IdentitiesOnly yes

Host resinvpn
User root
Port 22222
ProxyCommand ssh -A core@manager.resin.io 'enter vpn docker exec -i resin-vpn nc %h %p'

Host resinstagingvpn
User root
Port 22222
ProxyCommand ssh -A core@manager.resinstaging.io 'enter vpn docker exec -i resin-vpn nc %h %p'
