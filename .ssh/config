 User root
 Port 22222
 ProxyCommand ssh -A core@manager.resin.io 'enter vpn "sudo nsenter --target \$(docker inspect --format {{.State.Pid}} resin-vpn) --mount --net nc %h %p"'
 StrictHostKeyChecking no
 UserKnownHostsFile /dev/null

